{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df-aeu-ett-dev-mktng4"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"SAPBW_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SAPBW'",
			"defaultValue": "Integrated Security=True;Data Source=SAPHANA;Initial Catalog=ILNDB"
		},
		"SAPBW_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SAPBW'"
		},
		"linkedService2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'linkedService2'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://saaeuettdevmmarketing4.dfs.core.windows.net/"
		},
		"SAPBW_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "azuser"
		},
		"linkedService2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://saaeuettdevmmarketing4.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "df-aeu-ett-dev-mktng4_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"linkedService2_accountKey": {
						"value": "[parameters('linkedService2_accountKey')]"
					},
					"SAPBW_connectionString": {
						"value": "[parameters('SAPBW_connectionString')]"
					},
					"SAPBW_password": {
						"value": "[parameters('SAPBW_password')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"linkedService2_properties_typeProperties_url": {
						"value": "[parameters('linkedService2_properties_typeProperties_url')]"
					},
					"SAPBW_properties_typeProperties_userName": {
						"value": "[parameters('SAPBW_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}