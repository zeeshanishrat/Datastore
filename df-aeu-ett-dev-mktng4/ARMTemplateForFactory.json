{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df-aeu-ett-dev-mktng4"
		},
		"SAPBW_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SAPBW'",
			"defaultValue": "Integrated Security=True;Data Source=SAPHANA;Initial Catalog=ILNDB"
		},
		"SAPBW_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SAPBW'"
		},
		"SAPBW_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "azuser"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/pipeline1')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Copy data1",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "0.12:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"source": {
								"type": "SqlServerSource",
								"queryTimeout": "02:00:00",
								"isolationLevel": "ReadCommitted",
								"partitionOption": "None"
							},
							"sink": {
								"type": "SqlServerSink",
								"writeBehavior": "insert",
								"sqlWriterUseTableLock": false
							},
							"enableStaging": false,
							"translator": {
								"type": "TabularTranslator",
								"typeConversion": true,
								"typeConversionSettings": {
									"allowDataTruncation": true,
									"treatBooleanAsNumber": false
								}
							}
						},
						"inputs": [
							{
								"referenceName": "SqlServerTable1",
								"type": "DatasetReference",
								"parameters": {}
							}
						],
						"outputs": [
							{
								"referenceName": "SqlServerTable1",
								"type": "DatasetReference",
								"parameters": {}
							}
						]
					}
				],
				"policy": {
					"elapsedTimeMetric": {}
				},
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/SqlServerTable1')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/SqlServerTable1')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "SAPBW",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "SqlServerTable",
				"schema": [
					{
						"name": "EQUIPO_ID",
						"type": "int",
						"precision": 10
					},
					{
						"name": "NOMBRE_EQUIPO",
						"type": "varchar"
					},
					{
						"name": "MARCA",
						"type": "int",
						"precision": 10
					},
					{
						"name": "SERIE",
						"type": "varchar"
					},
					{
						"name": "LINEA",
						"type": "int",
						"precision": 10
					},
					{
						"name": "LUBRICACION",
						"type": "bit"
					},
					{
						"name": "INGRESADOPOR",
						"type": "int",
						"precision": 10
					},
					{
						"name": "MODIFICADOPOR",
						"type": "int",
						"precision": 10
					},
					{
						"name": "FECHAINGRESO",
						"type": "smalldatetime",
						"precision": 16,
						"scale": 0
					},
					{
						"name": "FECHAMODIFICACION",
						"type": "smalldatetime",
						"precision": 16,
						"scale": 0
					}
				],
				"typeProperties": {
					"schema": "dbo",
					"table": "TBL_EQUIPOS"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/SAPBW')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/SAPBW')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "SqlServer",
				"typeProperties": {
					"connectionString": "[parameters('SAPBW_connectionString')]",
					"userName": "[parameters('SAPBW_properties_typeProperties_userName')]",
					"password": {
						"type": "SecureString",
						"value": "[parameters('SAPBW_password')]"
					}
				},
				"connectVia": {
					"referenceName": "SAPBWIntigrationRunTime",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/integrationRuntimes/SAPBWIntigrationRunTime')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/SAPBWIntigrationRunTime')]",
			"type": "Microsoft.DataFactory/factories/integrationRuntimes",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "SelfHosted",
				"typeProperties": {
					"selfContainedInteractiveAuthoringEnabled": false
				}
			},
			"dependsOn": []
		}
	]
}